<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="MealBase">
    <defName>LunchboxMeal</defName>
    <label>lunchbox Meal</label>
    <description>A high-quality packaged lavish meal, manufactured for use while traveling. Does not rot nor deteriorate.</description>
    <graphicData>
      <texPath>LunchboxFull</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
	  <drawSize>0.90</drawSize>
    </graphicData>
    <statBases>
      <DeteriorationRate>0</DeteriorationRate>
      <MarketValue>45</MarketValue>
      <Mass>0.4</Mass>
      <WorkToMake>750</WorkToMake>
      <Nutrition>1.0</Nutrition>
    </statBases>
    <ingestible>
      <preferability>MealLavish</preferability>
			<tasteThought>AteLavishLunchboxMeal</tasteThought>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
  </ThingDef>
  
</Defs>